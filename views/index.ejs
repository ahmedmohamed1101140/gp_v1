<%include ./partials/header%>

<header class="jumbotron">
    <div class="container">
        <h1>FCIS Learning Managment System</h1>
        <p>platform that conect all univercity members and provide online services </p>
    </div>
</header>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<style>
    .pwhite{color:white; }
</style>

<div class="container">
    <h2>Welcome to Ainshams University</h2>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>
  
      <!-- Wrapper for slides -->
      <div class="carousel-inner">
  
        <div class="item active">
          <img style="width:100%; height:600px;" src="userimage/studnets.JPG" alt="Los Angeles" style="width:100%;">
          <div class="carousel-caption">
            <h3>Students</h3>
            <p class="pwhite"> we always have so much fun</p>
          </div>
        </div>
  
        <div class="item">
          <img style="width:100%; height:600px;" src="userimage/doctor.jpg" alt="Chicago" style="width:100%;">
          <div class="carousel-caption">
            <h3>teaching staff</h3>
            <p class="pwhite">we have the best teaching staff</p>
          </div>
        </div>
      
        <div class="item">
          <img style="width:100%; height:600px;"src="userimage/labs.jpg" alt="New York" style="width:100%;">
          <div class="carousel-caption">
            <h3>labs and classes </h3>
            <p class="pwhite">
                Laboratory equipped with the latest equipment</p>
          </div>
        </div>
    
      </div>
  
      <!-- Left and right controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
</div>


<%include ./partials/footer%>




    