
<%include ../partials/header%>
<div class="container">

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">

                    <div class="header">
                        <h4 class="title">Add new Users</h4>
                        <br>
                    </div>

                    <div class="content">
                        <form action="/Users/seedusers" method="POST">
                            <div class="row">
                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="dep_name">Number of students</label>
                                        <input placeholder="Number of students" name="studentscount" type="text" class="form-control" required="required">
                                    </div>
                                </div>



                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="dep_key">Department serial</label>
                                        <input placeholder="Department serial"  id="collage_serial" type="text" name="collage_serial"
                                               class="form-control"
                                               required="required">
                                    </div>
                                </div>
                            </div>

                            <br><br>
                            <div class="row">
                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label class=" control-label">Choose department</label>
                                        <div class="">
                                            <select  name="departemnt_name" id="basic" class="selectpicker show-tick form-control" data-live-search="true">
                                                <option value="sw"> sw </option>
                                                <option value="bio">bio</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="dep_date"> Year </label>
                                        <input id="dep_date" name="year" type="text" placeholder="Ex:2014"
                                               class="form-control"
                                               required="required">
                                    </div>
                                </div>
                            </div>
                            <br><br><br>

                            <br><br>
                            <div class="row">
                                <div class="col-md-8">
                                    <button class="btn btn-info btn-fill pull-left" type="submit">Add Users
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>





</div>

<%include ../partials/footer%>