<% include ../partials/header %>
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">

                    <div class="header">
                        <h4 class="title">Create New Department</h4>
                    </div>

                    <div class="content">

                        <form class="col-md-8" action="/departments" method="POST" enctype="multipart/form-data">

                            <div class="row">

                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="dep_name">Department Name</label>
                                        <input placeholder="Department Name" name="dep_name" type="text"
                                               class="form-control"
                                               required="required">
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="dep_key">Department Key</label>
                                        <input placeholder="Department Key" id="dep_key" type="text" name="dep_key"
                                               class="form-control"
                                               required="required">
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="textarea1">Department Description</label>
                                        <textarea placeholder="Description" id="textarea1" name="dep_description"
                                                  class="form-control"
                                                  data-length="400" required="required"></textarea>
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="admins">Choose Department Admin</label>
                                        <br>
                                        <select id="admins">
                                            <option name="admin" value="v0" disabled selected>Choose Departmen Admin
                                            </option>
                                            <option name="admin" value="v1"
                                                    data-icon="data:image/png;base64,"
                                                    class="left circle">Admin 1
                                            </option>
                                            <option name="admin" value="v2"
                                                    data-icon="data:image/png;base64,"
                                                    class="left circle">Admin 2
                                            </option>
                                            <option name="admin" value="v3"
                                                    data-icon="data:image/png;base64,"
                                                    class="left circle">Admin 3
                                            </option>
                                        </select>
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="dep_date">Department Opened Since</label>
                                        <input id="dep_date" name="dep_date" type="number" placeholder="Ex:2014"
                                               class="form-control"
                                               required="required">
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="depart_desc">Department Description File</label>
                                        <input id="depart_desc" class="form-control" name="my_file" type="file"
                                               required="required">
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="courses_desc">Courses Description File</label>
                                        <input id="courses_desc" class="form-control" name="my_file" type="file"
                                               required="required">
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="dep_logo">Department logo</label>
                                        <input id="dep_logo" class="form-control" name="my_file" type="file"
                                               required="required">
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <button class="btn btn-info btn-fill pull-left" type="submit">Create Department
                                    </button>
                                </div>

                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<% include ../partials/footer %>