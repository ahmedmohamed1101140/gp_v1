 <script type="text/javascript">
  $(document).ready(function() {
        $('body').confirmation({
            selector: '[data-toggle="confirmation"]',
            onConfirm: function(event, element) {document.getElementById("delete_form").submit() },

        });

        $('.confirmation-callback').confirmation({
            onConfirm: function(event, element) {document.getElementById("form").submit() },
            onCancel: function(event, element) { },
            btnOkLabel:"yes",
            btnOkClass:"btn btn-sm btn-primary"
        });
    });
    $(document).ready(function() {
        var table = $('.example').DataTable( {
                select: {
                    style:    'multi'
                },
                lengthChange: true,
                buttons: 
                [ 'copy', 'csv', 'excel', 'pdf', 'print', 
                    {
                        text: 'Delete Selected',
                        style: "btn btn-danger",
                        action: function ( e, dt, node, config ) {
                            alert( 'Button activated' );
                        }
                    }   
                ]
            } 
        );
        table.buttons().container().appendTo( '#example_wrapper .col-sm-6:eq(0)');

    } );    


    $(document).ready(function () {
        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
        });
    });


    $(document).ready(function() {
        $('#summernote').summernote({
            height: 200
        });
    });


    
  var searchbar = document.getElementById('search');
var posts=document.querySelectorAll('#posts');
searchbar.addEventListener('keyup',function (e) {

    const term=e.target.value.toLowerCase();

    for (var i = 0, len = posts.length; i < len; i++) {
        if (posts[i].textContent.toLocaleLowerCase().indexOf(term)!=-1)
        {
            posts[i].style.display='block';

        }
        else{
            posts[i].style.display='none'
        }
    }

})

  
 </script>

 


